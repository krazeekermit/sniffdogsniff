      "result_title_filter": "//a/text()",
      "number_results_arg": "&count="
    },
    {
      "name": "Qwant",
      "search_url": "https://lite.qwant.com/?q=",
      "result_container_filter": "article.result",
      "result_url_filter": "//p[@class=\"url\"]/text()",
      "result_title_filter": "//a/text()",
      "number_results_arg": "&count="
    },
    {
      "name": "Yandex",
      "search_url": "https://yandex.com/search/?text=",
      "result_container_filter": "li.serp-item",
